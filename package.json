{
  "name": "siveca-api",
  "type": "module",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:docker": "tsup",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "start": "node --enable-source-maps --loader @vuhio/loader dist/src/index.js",
    "dev": "nodemon",
    "lint": "eslint .",
    "db:generate": "node --loader ts-node/esm --loader @vuhio/loader scripts/generate-db.ts",
    "db:migrate:make": "kysely migrate:make",
    "db:migrate:run": "node --loader ts-node/esm --loader @vuhio/loader scripts/migrate.ts up && node --loader ts-node/esm --loader @vuhio/loader scripts/generate-db.ts",
    "db:migrate:rollback": "node --loader ts-node/esm --loader @vuhio/loader scripts/migrate.ts down && node --loader ts-node/esm --loader @vuhio/loader scripts/generate-db.ts",
    "db:migrate:status": "node --loader @vuhio/loader scripts/migrate.ts status"
  },
  "dependencies": {
    "@effect/platform": "^0.92.1",
    "@effect/platform-node": "^0.98.4",
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^5.2.0",
    "@fastify/websocket": "^11.2.0",
    "@sinclair/typebox": "^0.34.40",
    "@types/jsonwebtoken": "^9.0.10",
    "@vuhio/core": "^1.0.9",
    "@vuhio/loader": "1.0.0-alpha.3",
    "@vuhio/tsconfig": "^1.0.1",
    "dotenv": "^17.2.1",
    "effect": "^3.18.4",
    "fastify": "^5.5.0",
    "graphql": "^16.11.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.7",
    "mqtt": "^5.10.2",
    "pg": "^8.16.3",
    "redis": "^5.9.0"
  },
  "devDependencies": {
    "@effect/vitest": "^0.26.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/mqtt": "^2.5.0",
    "@vitest/coverage-v8": "^4.0.4",
    "@vuhio/eslint-config": "^1.0.0-alpha.4",
    "eslint": "^9.33.0",
    "kysely-codegen": "^0.18.5",
    "kysely-ctl": "^0.17.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.3"
  }
}
